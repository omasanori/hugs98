s%@HUGSDIR@%$HUGSDIR%g
s%@BINDIR@%$BINDIR%g
s%@SET_INSTALLDIRS@%$SET_INSTALLDIRS%g
s%@EXE@%$EXE%g
s%@OBJ@%$OBJ%g
