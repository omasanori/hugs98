  *-*-darwin*)
     CFLAGS="$CFLAGS -no-cpp-precomp"
     LDFLAGS="$LDFLAGS -flat_namespace"
     ;;
  # As suggested by James B. White III (Trey) <whitejbiii@ornl.gov> (for
  # AIX 4.3.3 & XL C 5.0.2 at least, may need to tweak the pattern match
  # below some to avoid upsetting other AIX versions).
  *-*-aix*)
     optcflags="-qalloca"
     CFLAGS_save="$CFLAGS"
     CFLAGS="$CFLAGS $optcflags"
     echo "$as_me:$LINENO: checking whether $CC accepts $optcflags" >&5
echo $ECHO_N "checking whether $CC accepts $optcflags... $ECHO_C" >&6
     ac_ext=c
ac_cpp='$CPP $CPPFLAGS'
